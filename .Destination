package tiketKereta;

public class Tujuan {
    private String nama;
    private Tiket ti[] = new Tiket[9];
    int counter=0;

    public Tujuan(String nama) {
        this.nama = nama;
    }

    public void setTi(Tiket ti) {
        this.ti[counter] = ti;
        counter ++; 
    }

    public String getNama() {
        return nama;
    }

    public Tiket getTi(int i) {
        return ti[i];
    }
     
}
