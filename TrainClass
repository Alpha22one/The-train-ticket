package tiketKereta;

public class Kelas {
   private String nama;
   private Tujuan tu[] = new Tujuan[3];
   int counter=0;

    public Kelas(String nama) {
        this.nama= nama;
    }


    public void setTu(Tujuan tu) {
        this.tu[counter] = tu;
        counter ++;  
    } 

    public String getNama() {
        return nama;
    }

    public Tujuan getTu(int i) {
        return tu[i];
    }
    
  
}
